{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "regions": ["iad1"],
  "functions": {
    "api/compute/**/*.ts": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 60,
      "includeFiles": "node_modules/{@0glabs/0g-serving-broker,eciesjs,@ecies/ciphers,crypto-js,tslib}/**"
    },
    "api/storage/**/*.ts": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 60
    }
  }
}

